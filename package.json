{
  "name": "orbio-game",
  "version": "1.0.0",
  "description": "Multiplayer .io-style game with blockchain integration",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "echo 'No build step required'"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "express-rate-limit": "^7.1.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
 "apiLimiter": "rateLimit" {
  "windowMs": "15 * 60 * 1000", 
  "standardHeaders": true,
  "max": 500, 
  "legacyHeaders": false,
  "message": "Too many requests from this IP, please try again after 15 minutes"
}   